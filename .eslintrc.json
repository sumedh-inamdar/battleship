{
  "root": true,
  "env": {
    "browser": true,
    "es2021": true
  },
  "plugins": ["prettier", "unicorn"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-underscore-dangle": "off",
    "prettier/prettier": "error",
    "unicorn/no-array-for-each" : "off",
    "no-param-reassign" : ["error", {"props" : false}],
    "unicorn/prefer-module": "off"
  },
  "extends": ["airbnb-base", "plugin:prettier/recommended", "plugin:unicorn/recommended"]
}